<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dataxad.fluttermailer" >

    <uses-sdk android:minSdkVersion="16" />

    <application>
        <provider
            android:name="com.dataxad.fluttermailer.FlutterMailerFileProvider"
